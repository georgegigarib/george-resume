{
  "name": "george-resume-presentation",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tests": "vitest --reporter default",
    "coverage": "vitest run --coverage",
    "test-report": "vitest --reporter default --reporter vitest-markdown-reporter run --coverage",
    "format": "prettier --write src/",
    "before-push": "bun lint && bun format && bun test-report",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.1.1",
    "@reduxjs/toolkit": "^2.2.8",
    "@testing-library/react": "^16.0.0",
    "@types/eslint__js": "^8.42.3",
    "clsx": "^2.1.1",
    "framer-motion": "^11.3.28",
    "i18next": "^23.15.1",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.2",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.0",
    "tailwind-merge": "^2.5.2",
    "vitest-markdown-reporter": "^0.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "2.1.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.0",
    "vitest": "^2.1.3"
  }
}
